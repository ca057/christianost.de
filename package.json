{
  "name": "christianost.de",
  "version": "1.0.0",
  "description": "christianost.de",
  "repository": "https://github.com/ca057/christianost.de",
  "author": "Christian Ost <hallo@christianost.de>",
  "license": "MIT",
  "scripts": {
    "clean": "hugo --gc && rimraf public",
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "hugo -vwDEF --gc",
    "start:webpack": "webpack-dev-server --config webpack.dev.js --hot",
    "preview": "run-p preview:**",
    "preview:hugo": "yarn run start:hugo -- -D -F",
    "preview:webpack": "yarn run start:webpack",
    "prebuild": "yarn run clean",
    "build": "yarn run build:webpack && yarn run build:hugo",
    "build:preview": "yarn run build:webpack && yarn run build:hugo:preview",
    "build:hugo": "hugo -v --minify --gc",
    "build:hugo:preview": "yarn run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "vanilla-lazyload": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "assets-webpack-plugin": "^3.9.12",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "exports-loader": "^0.7.0",
    "file-loader": "^4.3.0",
    "hugo-bin": "^0.56.3",
    "imports-loader": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "plugin-error": "^1.0.1",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "rimraf": "^2.7.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "svg-to-png": "^4.0.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.3.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.0.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}
